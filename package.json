{
  "name": "plang",
  "version": "0.1.2",
  "description": "A general purpose, statically-typed, interpreted language.",
  "displayName": "PLang",
  "icon": "https://avatars.githubusercontent.com/u/146694713?s=400&u=4e01b5afefbf7e40641702e5cbdf785ff183fe35&v=4",
  "main": "dist/src/main.js",
  "bin": {
    "pint": "dist/tools/pint.js",
    "prepl": "dist/tools/repl.js"
  },
  "scripts": {
    "test": "mocha dist/tools/unit-tests -j 4 --extension js",
    "build": "tsc",
    "repl": "node dist/tools/repl.js",
    "pint": "node dist/tools/pint.js",
    "path": "tsc && npm link",
    "docs": "npx typedoc --options typedoc.config.json",
    "push": "tsc --skipLibCheck && git add . && git commit -m 'docs: (autogenerate)' && git push -u"
  },
  "keywords": [
    "p",
    "plang",
    "language"
  ],
  "author": "KevinAlavik, R-unic",
  "contributors": [
    "runicly",
    "kevinalavik"
  ],
  "license": "LGPL-2.1-or-later",
  "dependencies": {
    "colors.ts": "^1.0.20",
    "mocha": "^10.2.0",
    "os": "^0.1.2",
    "readline-sync": "^1.4.10",
    "should": "^13.2.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.0",
    "@types/readline-sync": "^1.4.5"
  }
}
